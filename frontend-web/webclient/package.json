{
  "name": "sducloud",
  "version": "0.13.10",
  "description": "SDU Cloud",
  "author": "IMADA - eScience Center, SDU",
  "jest": {
    "collectCoverage": true,
    "coverageDirectory": "<rootDir>/coverage",
    "coverageReporters": [
      "html",
      "cobertura"
    ],
    "modulePaths": [
      "app"
    ],
    "transform": {
      "^.+\\.tsx?$": "ts-jest",
      "^.+\\.js$": "babel-jest"
    },
    "moduleNameMapper": {
      "^.+\\.png$": "file-loader",
      "^.+\\.ttf$": "file-loader",
      "Authentication/SDUCloudObject": "<rootDir>/__tests__/mock/Cloud.ts"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)\\.(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "preset": "ts-jest",
    "testMatch": null,
    "globals": {
      "ts-jest": {
        "ts-config": "tsconfig.json"
      }
    }
  },
  "scripts": {
    "start": "webpack-dev-server --config webpack.dev.js --inline --progress --port 9000",
    "clean": "rm -rf dist/",
    "build": "npm run clean && npm run prepare-icons && webpack --config webpack.prod.js --progress --profile --bail",
    "install": "napa && npm run prepare-icons",
    "test": "npm run prepare-icons && jest --no-cache",
    "prepare-icons": "npm run icons-clean && npm run icons-components && npm run icons-index",
    "icons-clean": "cd app/ui-components/icons; rm -rf components index.ts",
    "icons-components": "cd app/ui-components/icons; svgr . --ext=tsx --filename-case camel -d components",
    "icons-index": "node scripts/create-icons-index.js"
  },
  "dependencies": {
    "@svgr/cli": "^4.1.0",
    "avataaars": "^1.2.1",
    "fuse.js": "^3.3.0",
    "install": "^0.12.2",
    "jsonwebtoken": "8.4.0",
    "moment": "~2.24.0",
    "moment-timezone": "^0.5.23",
    "npm": "^6.7.0",
    "prop-types": "15.6.2",
    "rc-slider": "^8.6.4",
    "react": "16.7.0",
    "react-datepicker": "2.0.0",
    "react-dom": "16.7.0",
    "react-dropzone": "^8.0.4",
    "react-markdown": "^4.0.6",
    "react-modal": "^3.8.1",
    "react-redux": "^6.0.0",
    "react-router": "4.3.1",
    "react-router-dom": "4.3.1",
    "recharts": "^1.4.2",
    "redux": "4.0.1",
    "redux-responsive": "^4.3.8",
    "styled-components": "^4.1.3",
    "styled-system": "^3.2.1",
    "svgi": "^1.1.0",
    "sweetalert2": "7.33.1"
  },
  "devDependencies": {
    "@types/history": "^4.7.2",
    "@types/recharts": "^1.1.10",
    "@babel/core": "^7.2.2",
    "@babel/plugin-proposal-class-properties": "^7.3.0",
    "@babel/plugin-proposal-decorators": "^7.3.0",
    "@babel/plugin-proposal-export-default-from": "^7.2.0",
    "@babel/plugin-proposal-export-namespace-from": "^7.2.0",
    "@babel/plugin-proposal-numeric-separator": "^7.2.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.3.1",
    "@babel/polyfill": "^7.2.5",
    "@babel/preset-env": "^7.3.1",
    "@babel/preset-react": "^7.0.0",
    "@babel/preset-typescript": "^7.1.0",
    "@types/enzyme": "^3.1.16",
    "@types/enzyme-adapter-react-16": "^1.0.3",
    "@types/jest": "23.3.13",
    "@types/json-schema": "^7.0.1",
    "@types/jsonwebtoken": "^8.3.0",
    "@types/moment-timezone": "^0.5.10",
    "@types/react": "^16.7.22",
    "@types/react-datepicker": "2.0.2",
    "@types/react-dom": "^16.0.11",
    "@types/react-modal": "3.8.0",
    "@types/react-redux": "^7.0.1",
    "@types/react-router": "^4.4.3",
    "@types/react-router-dom": "^4.3.1",
    "@types/react-test-renderer": "16.0.3",
    "@types/styled-components": "4.1.6",
    "@types/styled-system": "^3.1.3",
    "babel-jest": "24.0.0",
    "babel-loader": "8.0.5",
    "babel-plugin-styled-components": "^1.10.0",
    "compression-webpack-plugin": "^2.0.0",
    "copy-webpack-plugin": "4.6.0",
    "css-loader": "^2.1.0",
    "empty-module": "~0.0.2",
    "enzyme": "^3.8.0",
    "enzyme-adapter-react-16": "^1.8.0",
    "file-loader": "3.0.1",
    "html-loader": "0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "imports-loader": "0.8.0",
    "jest": "^24.0.0",
    "jest-styled-components": "^6.3.1",
    "mini-css-extract-plugin": "^0.5.0",
    "napa": "3.0.0",
    "raw-loader": "1.0.0",
    "react-test-renderer": "16.7.0",
    "redux-devtools-extension": "^2.13.7",
    "source-map-loader": "^0.2.4",
    "style-loader": "0.23.1",
    "terser-webpack-plugin": "^1.2.1",
    "ts-jest": "^23.10.5",
    "ts-loader": "^5.3.3",
    "typescript": "3.2.4",
    "url-loader": "1.1.2",
    "webpack": "4.29.0",
    "webpack-cli": "^3.2.1",
    "webpack-dev-server": "3.1.14",
    "webpack-merge": "4.2.1"
  }
}
