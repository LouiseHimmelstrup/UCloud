        "react-syntax-highlighter": "15.5.0",
        "react-transition-group": "4.4.5",
        "react-virtualized-auto-sizer": "1.0.20",
        "react-window": "1.8.9",
        "recharts": "2.1.13",
        "redux": "4.2.1",
        "svgi": "1.1.2",
        "ua-parser-js": "1.0.36",
        "xterm": "5.2.1",
        "xterm-addon-fit": "0.7.0"
