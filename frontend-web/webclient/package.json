        "@rjsf/core": "4.2.3",
        "@svgr/cli": "5.5.0",
        "apexcharts": "^3.44.0",
        "assert": "2.1.0",
        "date-fns": "2.30.0",
        "deepcopy": "^2.1.0",
        "fast-deep-equal": "3.1.3",
        "fuse.js": "7.0.0",
        "jsrsasign": "10.8.6",
        "localforage": "1.10.0",
        "magic-bytes.js": "^1.5.0",
        "path-browserify": "1.0.1",
        "react": "18.2.0",
        "react-apexcharts": "^1.4.1",
        "react-datepicker": "4.21.0",
