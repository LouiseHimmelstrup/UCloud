{
    "name": "ucloud",
    "version": "2022.1.22",
    "description": "UCloud",
    "author": "IMADA - eScience Center, SDU",
    "scripts": {
        "start": "vite",
        "start:local": "vite --mode local-dev",
        "start:compose": "vite --mode compose",
        "watch": "./node_modules/typescript/bin/tsc --watch --noEmit",
        "build": "node scripts/update-version-file.js && vite build",
        "serve": "vite preview",
        "icons-clean": "cd app/ui-components/icons; rm -rf components index.ts",
        "icons-components": "cd app/ui-components/icons; svgr . --ext=tsx --filename-case camel -d components && rm -f components/index.tsx",
        "icons-index": "node scripts/create-icons-index.js"
    },
    "dependencies": {
        "@ginkgo-bioworks/react-json-schema-form-builder": "2.6.3",
        "@novnc/novnc": "1.2.0",
        "@rjsf/core": "^3.2.1",
        "@svgr/cli": "5.5.0",
        "assert": "2.0.0",
        "date-fns": "2.27.0",
        "fast-deep-equal": "3.1.3",
        "fuse.js": "6.4.6",
        "localforage": "1.10.0",
        "path-browserify": "1.0.1",
        "react": "17.0.2",
        "react-datepicker": "3.8.0",
        "react-dom": "17.0.2",
        "react-json-view": "1.21.3",
        "react-markdown": "7.1.1",
        "react-modal": "3.14.4",
        "react-redux": "7.2.6",
        "react-router": "5.2.1",
        "react-router-dom": "5.3.0",
        "react-syntax-highlighter": "15.4.5",
        "react-transition-group": "4.4.2",
        "recharts": "2.1.6",
        "redux": "4.1.2",
        "redux-responsive": "4.3.8",
        "styled-components": "5.3.3",
        "styled-system": "5.1.5",
        "svgi": "1.1.0",
        "ua-parser-js": "1.0.2",
        "vfile": "5.2.0",
        "xterm": "4.15.0",
        "xterm-addon-fit": "0.5.0"
    },
    "devDependencies": {
        "@types/history": "4.7.10",
        "@types/json-schema": "7.0.9",
        "@types/react": "17.0.38",
        "@types/react-datepicker": "4.3.4",
        "@types/react-dom": "17.0.11",
        "@types/react-dropzone": "5.1.0",
        "@types/react-modal": "3.13.1",
        "@types/react-redux": "7.1.22",
        "@types/react-router": "5.1.18",
        "@types/react-router-dom": "5.3.3",
        "@types/react-transition-group": "4.4.4",
        "@types/recharts": "1.8.23",
        "@types/styled-components": "5.1.21",
        "@types/styled-system": "5.1.15",
        "@types/ua-parser-js": "0.7.36",
        "@vitejs/plugin-react-refresh": "1.3.6",
        "redux-devtools-extension": "2.13.9",
        "ts-loader": "9.2.6",
        "typescript": "4.5.5",
        "vite": "2.7.13"
    }
}
