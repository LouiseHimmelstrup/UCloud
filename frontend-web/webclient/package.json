{
  "name": "sducloud",
  "version": "0.8.0",
  "description": "SDU Cloud",
  "author": "IMADA - eScience Center, SDU",
  "jest": {
    "collectCoverage": true,
    "coverageDirectory": "<rootDir>/coverage",
    "coverageReporters": [
      "html",
      "cobertura"
    ],
    "modulePaths": [
      "app"
    ],
    "transform": {
      "^.+\\.tsx?$": "ts-jest",
      "^.+\\.js$": "babel-jest"
    },
    "moduleNameMapper": {
      "^.+\\.png$": "file-loader",
      "Authentication/SDUCloudObject": "<rootDir>/__tests__/mock/Cloud.ts"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)\\.(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "globals": {
      "ts-jest": {
        "ts-config": "tsconfig.json"
      }
    }
  },
  "scripts": {
    "start": "webpack-dev-server --config webpack.dev.js --inline --progress --port 9000",
    "clean": "rm -rf dist/",
    "build": "npm run clean && npm run prepare-icons && webpack --config webpack.prod.js --progress --profile --bail",
    "install": "napa && npm run prepare-icons",
    "test": "npm run prepare-icons && jest",
    "prepare-icons": "npm run icons-clean && npm run icons-components && npm run icons-index",
    "icons-clean": "cd app/ui-components/icons; rm -rf components index.ts",
    "icons-components": "cd app/ui-components/icons; svgr . --ext=tsx --filename-case camel -d components",
    "icons-index": "node scripts/create-icons-index.js"
  },
  "dependencies": {
    "@svgr/cli": "^4.1.0",
    "@types/recharts": "^1.1.5",
    "avataaars": "^1.2.1",
    "fuse.js": "^3.3.0",
    "install": "^0.12.2",
    "jsonwebtoken": "8.4.0",
    "loaders.css": "0.1.2",
    "moment": "~2.23.0",
    "moment-timezone": "^0.5.23",
    "npm": "^6.5.0",
    "prop-types": "15.6.2",
    "rc-slider": "^8.6.4",
    "react": "16.6.3",
    "react-datepicker": "2.0.0",
    "react-dom": "16.6.3",
    "react-dropzone": "^8.0.2",
    "react-markdown": "^4.0.4",
    "react-modal": "^3.7.1",
    "react-redux": "^6.0.0",
    "react-router": "4.3.1",
    "react-router-dom": "4.3.1",
    "recharts": "^1.4.1",
    "redux": "4.0.1",
    "styled-components": "^4.1.3",
    "styled-system": "^3.1.11",
    "svgi": "^1.1.0",
    "sweetalert2": "7.32.4"
  },
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@types/enzyme": "^3.1.15",
    "@types/enzyme-adapter-react-16": "^1.0.3",
    "@types/jest": "23.3.10",
    "@types/json-schema": "^7.0.1",
    "@types/jsonwebtoken": "^8.3.0",
    "@types/moment-timezone": "^0.5.9",
    "@types/react": "^16.7.17",
    "@types/react-datepicker": "2.0.1",
    "@types/react-dom": "^16.0.11",
    "@types/react-modal": "3.6.0",
    "@types/react-redux": "^6.0.11",
    "@types/react-router": "^4.4.3",
    "@types/react-router-dom": "^4.3.1",
    "@types/react-test-renderer": "16.0.3",
    "@types/styled-components": "4.1.4",
    "@types/styled-system": "^3.0.9",
    "babel-jest": "23.6.0",
    "babel-loader": "8.0.4",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-transform-runtime": "6.23.0",
    "babel-preset-env": "1.7.0",
    "babel-preset-es2015": "6.24.1",
    "babel-preset-react": "6.24.1",
    "babel-preset-stage-0": "6.24.1",
    "copy-webpack-plugin": "4.6.0",
    "css-loader": "^2.0.1",
    "empty-module": "~0.0.2",
    "enzyme": "^3.8.0",
    "enzyme-adapter-react-16": "^1.7.1",
    "file-loader": "2.0.0",
    "html-loader": "0.5.5",
    "html-webpack-plugin": "3.2.0",
    "imports-loader": "0.8.0",
    "jest": "^23.6.0",
    "jest-styled-components": "^6.3.1",
    "mini-css-extract-plugin": "^0.5.0",
    "napa": "3.0.0",
    "node-sass": "4.11.0",
    "phantomjs-prebuilt": "2.1.16",
    "raw-loader": "1.0.0",
    "react-test-renderer": "16.6.3",
    "sass-loader": "^7.1.0",
    "style-loader": "0.23.1",
    "ts-jest": "^23.10.5",
    "ts-loader": "^5.3.1",
    "typescript": "3.1.6",
    "uglifyjs-webpack-plugin": "^1.3.0",
    "url-loader": "1.1.2",
    "webpack": "4.27.1",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "3.1.10",
    "webpack-merge": "4.1.5"
  }
}
