package = sqlite3
headers = sqlite3.h
staticLibraries = libsqlite3.a
libraryPaths = vendor/libsqlite3/linux
linkerOpts = -ldl

---

SQLITE_API int sqlite3_bind_text_kt_fix(
    sqlite3_stmt* stmt,
    int param,
    const void* data,
    int data_size,
    void (*destructor)(void *)
) {
    return sqlite3_bind_text(stmt, param, data, data_size, destructor);
}
