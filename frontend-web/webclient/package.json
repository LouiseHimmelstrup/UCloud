{
  "name": "sducloud",
  "version": "0.3.1",
  "description": "SDU Cloud",
  "author": "IMADA - eScience Center, SDU",
  "jest": {
    "collectCoverage": true,
    "coverageDirectory": "<rootDir>/coverage",
    "coverageReporters": [
      "text",
      "html",
      "cobertura"
    ],
    "modulePaths": [
      "app"
    ],
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "moduleNameMapper": {
      "^.+\\.png$": "file-loader",
      "Authentication/SDUCloudObject": "<rootDir>/__tests__/mock/Cloud.ts"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)\\.(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  },
  "scripts": {
    "start": "webpack-dev-server --config webpack.dev.js --inline --progress --port 9000",
    "clean": "rm -rf dist/",
    "build": "npm run clean && webpack --config webpack.prod.js --progress --profile --bail",
    "install": "napa",
    "test": "jest"
  },
  "dependencies": {
    "avataaars": "^1.2.0",
    "jsonwebtoken": "8.3.0",
    "loaders.css": "0.1.2",
    "moment": "~2.22.2",
    "moment-timezone": "^0.5.21",
    "npm": "6.4.1",
    "prop-types": "15.6.2",
    "react": "16.5.1",
    "react-datepicker": "^1.6.0",
    "react-dom": "16.5.1",
    "react-dropzone": "^5.1.0",
    "react-markdown": "^3.6.0",
    "react-redux": "^5.0.7",
    "react-router": "4.3.1",
    "react-router-dom": "4.3.1",
    "redux": "4.0.0",
    "semantic-ui-css": "^2.4.0",
    "semantic-ui-react": "^0.82.5",
    "sweetalert2": "7.26.29",
    "uppy": "0.23.3"
  },
  "devDependencies": {
    "@babel/core": "^7.0.1",
    "@types/enzyme": "^3.1.14",
    "@types/enzyme-adapter-react-16": "^1.0.3",
    "@types/jest": "^23.3.2",
    "@types/react": "^16.4.14",
    "@types/react-datepicker": "^1.1.7",
    "@types/react-dom": "^16.0.7",
    "@types/react-redux": "^6.0.9",
    "@types/react-router": "^4.0.31",
    "@types/react-router-dom": "^4.3.1",
    "@types/react-test-renderer": "16.0.2",
    "babel-jest": "23.6.0",
    "babel-loader": "8.0.2",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-transform-runtime": "6.23.0",
    "babel-preset-env": "1.7.0",
    "babel-preset-es2015": "6.24.1",
    "babel-preset-react": "6.24.1",
    "babel-preset-stage-0": "6.24.1",
    "copy-webpack-plugin": "4.5.2",
    "css-loader": "^1.0.0",
    "empty-module": "~0.0.2",
    "enzyme": "^3.6.0",
    "enzyme-adapter-react-16": "^1.5.0",
    "file-loader": "2.0.0",
    "html-loader": "0.5.5",
    "html-webpack-plugin": "3.2.0",
    "imports-loader": "0.8.0",
    "jest": "^23.6.0",
    "mini-css-extract-plugin": "^0.4.2",
    "napa": "3.0.0",
    "node-sass": "^4.9.3",
    "phantomjs-prebuilt": "2.1.16",
    "raw-loader": "0.5.1",
    "react-test-renderer": "16.5.1",
    "sass-loader": "^7.1.0",
    "style-loader": "0.23.0",
    "ts-jest": "^23.1.4",
    "ts-loader": "^5.1.1",
    "typescript": "3.0.3",
    "uglifyjs-webpack-plugin": "^1.3.0",
    "url-loader": "1.1.1",
    "webpack": "4.19.0",
    "webpack-cli": "^3.1.0",
    "webpack-dev-server": "3.1.8",
    "webpack-merge": "4.1.4"
  }
}
