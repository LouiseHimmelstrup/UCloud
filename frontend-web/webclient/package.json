{
    "name": "ucloud",
    "version": "0.50.0-storage6",
    "description": "UCloud",
    "author": "IMADA - eScience Center, SDU",
    "scripts": {
        "start": "webpack serve --config webpack.dev.js --inline --progress --port 9000 --host 0.0.0.0",
        "start_use_local_backend": "npm run start -- --env local_dev",
        "start_use_compose_backend": "npm run start -- --env compose",
        "clean": "rm -rf dist/",
        "build": "npm run clean && node scripts/update-version-file.js && webpack --config webpack.prod.js --progress",
        "test": "jest --runInBand",
        "prepare-icons": "npm run icons-clean && npm run icons-components && npm run icons-index",
        "icons-clean": "cd app/ui-components/icons; rm -rf components index.ts",
        "icons-components": "cd app/ui-components/icons; svgr . --ext=tsx --filename-case camel -d components && rm -f components/index.tsx",
        "icons-index": "node scripts/create-icons-index.js"
    },
    "dependencies": {
        "@ginkgo-bioworks/react-json-schema-form-builder": "^2.4.2",
        "@novnc/novnc": "^1.2.0",
        "@rjsf/core": "^2.5.1",
        "@svgr/cli": "^5.5.0",
        "date-fns": "^2.22.1",
        "fast-deep-equal": "^3.1.3",
        "fuse.js": "^6.4.6",
        "localforage": "^1.9.0",
        "path-browserify": "^1.0.1",
        "react": "17.0.2",
        "react-datepicker": "3.8.0",
        "react-dom": "17.0.2",
        "react-dropzone": "^11.3.2",
        "react-markdown": "^5.0.3",
        "react-modal": "^3.14.2",
        "react-redux": "^7.2.4",
        "react-router": "5.2.0",
        "react-router-dom": "5.2.0",
        "react-syntax-highlighter": "^15.4.3",
        "react-transition-group": "^4.4.2",
        "recharts": "^2.0.9",
        "redux": "4.1.0",
        "redux-responsive": "^4.3.8",
        "styled-components": "^5.3.0",
        "styled-system": "^5.1.5",
        "svgi": "^1.1.0",
        "ua-parser-js": "^0.7.28",
        "xterm": "^4.12.0",
        "xterm-addon-fit": "^0.5.0"
    },
    "devDependencies": {
        "@testing-library/dom": "^8.1.0",
        "@testing-library/react": "^12.0.0",
        "@types/history": "^4.7.9",
        "@types/jest": "26.0.24",
        "@types/json-schema": "^7.0.8",
        "@types/react": "17.0.14",
        "@types/react-datepicker": "4.1.3",
        "@types/react-dom": "^17.0.9",
        "@types/react-dropzone": "^5.1.0",
        "@types/react-modal": "3.12.0",
        "@types/react-redux": "^7.1.16",
        "@types/react-router": "^5.1.15",
        "@types/react-router-dom": "^5.1.7",
        "@types/react-test-renderer": "17.0.1",
        "@types/react-transition-group": "^4.4.2",
        "@types/recharts": "^2.0.0",
        "@types/styled-components": "5.1.5",
        "@types/styled-system": "^5.1.12",
        "@types/ua-parser-js": "^0.7.36",
        "@typescript-eslint/eslint-plugin": "^4.28.2",
        "@typescript-eslint/parser": "^4.28.2",
        "compression-webpack-plugin": "^8.0.1",
        "copy-webpack-plugin": "9.0.1",
        "css-loader": "^5.2.6",
        "empty-module": "~0.0.2",
        "eslint": "^7.30.0",
        "eslint-plugin-react": "^7.24.0",
        "file-loader": "6.2.0",
        "html-webpack-plugin": "^5.3.2",
