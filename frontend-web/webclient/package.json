{
    "name": "ucloud",
    "version": "2022.2.29",
    "description": "UCloud",
    "author": "IMADA - eScience Center, SDU",
    "scripts": {
        "start": "vite",
        "start:local": "vite --mode local-dev",
        "start:compose": "vite --mode compose",
        "watch": "./node_modules/typescript/bin/tsc --watch --noEmit",
        "build": "node scripts/update-version-file.js && vite build",
        "serve": "vite preview",
        "icons-clean": "cd app/ui-components/icons; rm -rf components index.ts",
        "icons-components": "cd app/ui-components/icons; svgr . --ext=tsx --filename-case camel -d components && rm -f components/index.tsx",
        "icons-index": "node scripts/create-icons-index.js"
    },
    "dependencies": {
        "@ginkgo-bioworks/react-json-schema-form-builder": "2.9.2",
        "@novnc/novnc": "1.3.0",
        "@rjsf/core": "^4.2.3",
        "@svgr/cli": "5.5.0",
        "assert": "2.0.0",
        "big-json-viewer": "^0.1.7",
        "date-fns": "2.29.3",
        "fast-deep-equal": "3.1.3",
        "fuse.js": "6.6.2",
        "jsrsasign": "^10.5.27",
        "localforage": "1.10.0",
        "path-browserify": "1.0.1",
        "react": "17.0.2",
        "react-datepicker": "4.8.0",
        "react-dom": "17.0.2",
        "react-markdown": "8.0.3",
        "react-modal": "3.15.1",
        "react-redux": "8.0.2",
        "react-router": "5.2.1",
        "react-router-dom": "5.3.0",
        "react-syntax-highlighter": "15.5.0",
        "react-transition-group": "4.4.5",
        "react-virtualized-auto-sizer": "^1.0.7",
        "react-window": "^1.8.7",
        "recharts": "2.1.13",
        "redux": "4.2.0",
        "redux-responsive": "4.3.8",
        "styled-components": "5.3.6",
        "styled-system": "5.1.5",
        "svgi": "1.1.1",
        "ua-parser-js": "1.0.2",
        "vfile": "5.3.4",
        "xterm": "4.19.0",
        "xterm-addon-fit": "0.5.0"
    },
    "devDependencies": {
        "@types/history": "5.0.0",
        "@types/json-schema": "7.0.11",
        "@types/jsrsasign": "^10.5.4",
        "@types/react": "17.0.43",
